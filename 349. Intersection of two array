//less optimal
class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        set<int> st;
        vector<int> ans;
        for(int i=0;i<nums1.size(); i++){
            for(int j=0; j<nums2.size(); j++){
                if(nums1[i] == nums2[j]){
                    st.insert(nums1[i]);
                }
            }
        }
        for(int x : st){
            ans.push_back(x);
        }
        return ans;
    }
};

//more optimal  T.C = O(m+n)  S.C = O(min(n,m))
class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> s(nums1.begin(), nums1.end());
        vector<int> ans;
        for(auto x : nums2){
            if(s.count(x)>0) {
                ans.push_back(x);
                s.erase(x);
            }
        }
        return ans;
    }
};
